    "def fizzbuzz(end_number):\n",
    "    for i in range(1, end_number +1):\n",
    "        if i % 3 == 0 and i % 5 == 0:\n",
    "            print(f\"{i}: FizzBuzz\")\n",
    "        elif i % 3 == 0:\n",
    "            print(f\"{i}: Fizz\")\n",
    "        elif i % 5 == 0:\n",
    "            print(f\"{i}: Buzz\")\n"
   ]
